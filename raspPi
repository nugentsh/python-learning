import RPi.GPIO as gpi
import time

gpi.setwarnings(False)
gpi.setmode(gpi.BCM)#

gpi.setup(11, gpi.IN)
gpi.setup(13, gpi.IN)

gpi.setup(18, gpi.OUT)
gpi.setup(20, gpi.OUT)


if gpi.input(11):
    print('button 1 triggered')
    gpi.output(18, True)
        else if gpi.input(13):
        gpi.output(20, True)
        print('button 2 triggered')
